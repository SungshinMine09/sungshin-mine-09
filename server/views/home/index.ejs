<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <title>✨성신마인 09번지🔮</title>
    <link rel="stylesheet" href="/css/slide.css" />
    <link rel="stylesheet" href="/css/style.css" />
    <link rel="stylesheet" href="/css/index.css" />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link href="https://fonts.googleapis.com/css2?family=Mochiy+Pop+One&display=swap" rel="stylesheet" />
  </head>

  <body>
    <div class="slide slide_wrap">
      <div class="slide_item item1"><img src="/images/banner1.png"></div>
      <div class="slide_item item2"><img src="/images/banner2.png"></div>
      <div class="slide_prev_button slide_button">💜</div>
      <div class="slide_next_button slide_button">💜</div>
      <ul class="slide_pagination"></ul>
    </div>
    <script src="/js/slide.js"></script>

    <div class="newhot_button">
      <ul>
        <li><button>✨ NEW 공동구매 ✨</button></li>
        <li><button>🔥 수정이들의 HOT픽 🔥</button></li>
      </ul>
    </div>

    <div class="item">
      <div>
        <div>

          <% Array.from(newGonggus).forEach( newGonggu => { %>
          <div>
            <a href="<%= `/CoBuyRoom/totalGonggu/${newGonggu.id}` %>" style="text-decoration: none;">
              <img />
              <p> <%= newGonggu.title %> </p>
            </a>
          </div>
          <% }); %>
          
        </div>

        <div>

          <div>
            <a>
              <img />
              <p>남색 학잠</p>
            </a>
          </div>
          
        </div>
      </div>
    </div>
  </body>
</html>
